{
  "name": "deanfeb/indodana-paylater-magento2-dev",
  "description": "Indodana Paylater plugin for Magento v2",
  "type":"magento2-module",
  "authors": [
    {
      "name": "Indodana",
      "email": "dev@indodana.com"
    },
    {
      "name": "Dian Febrianto",
      "email": "dian.febrianto@gmail.com"
    }
    ],
  "repositories": [
    {
      "type": "path",
      "url": "packages/common",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "indodana/indodana-paylater-sdk": "2.0.0",
    "respect/validation": "^1.1"
    
  },
  "autoload": {
    "files": [
      "app/code/Indodana/PayLater/registrations.php"
    ],
    "psr-4": {
      "IndodanaCommon\\":"packages/src/",
      "Indodana\\PayLater\\":"app/code/Indodana/PayLater"
    }
  }
}
